<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regression" verbose="10">
    <listeners>
        <!-- Generates Test Suite execution reports: -->
        <listener class-name="org.testng.reporters.SuiteHTMLReporter"/>
    </listeners>
    <test name="Authentication tests (Register, Login)">
        <classes>
            <class name="tests.AuthTest"/>
        </classes>
    </test>
    <test name="Users tests">
        <classes>
            <class name="tests.UsersTest"></class>
        </classes>
    </test>
</suite>
